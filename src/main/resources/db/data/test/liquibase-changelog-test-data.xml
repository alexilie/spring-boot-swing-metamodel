<databaseChangeLog
		xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

	<changeSet id="release-test-data" author="EOrganism Test Loader" runOnChange="true" context="test-data">
		<comment>Create release table test data</comment>
		<loadUpdateData file="csv/release.csv"
						primaryKey="id"
						tableName="release"
						relativeToChangelogFile="true"
						separator=";">
		</loadUpdateData>
	</changeSet>
